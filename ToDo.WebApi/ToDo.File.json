							"// Generate and set escaped random lorem paragraphs to environment variable",
							"const randomLoremParagraphs = pm.variables.replaceIn('{{$randomLoremParagraphs}}').replace(/\\n/g, '\\\\n').replace(/\\r/g, '\\\\r');",
